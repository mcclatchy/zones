{
  "pageType": "story",
  "base": ".story-body [id^=zone-el]",
  "ignore": ["zone-el-16"],
  "zones": [
    {
      "id": "zone-moneycom",
      "filters": [
        {
          "type": "config",
          "name": "zone.moneycom",
          "value": true
        },
        {
          "type": "config",
          "name": "articleCredit",
          "value": "^((?!Paradise Media|Bankrate).)*$"
        }
      ],
      "zephr": {
        "feature": "zone-moneycom",
        "dataset": []
      },
      "placement": {
        "type": "after",
        "value": "zone-el-101"
      }
    },
    {
      "id": "zone-featured-carousel",
      "filters": [
        {
          "type": "subscriber",
          "value": true
        }
      ],
      "param": {
        "name": "zone.carousel",
        "value": true
      },
      "zephr": {
        "feature": "zone-featured-carousel",
        "dataset": [
          {
            "name": "taxonomy",
            "config": "marketInfo.taxonomy"
          },
          {
            "name": "market",
            "config": "marketInfo.domain"
          }
        ]
      },
      "placement": {
        "type": "before",
        "value": "zone-el-101"
      }
    }
  ]
}
