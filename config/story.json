{
  "wps": ".story-body [id^=zone-el]",
  "ignore": ["zone-el-16"],
  "cadence": {
    "subscriber": 5,
    "dma": 3,
    "standard": 3
  },
  "zones": [
    {
      "id": "zone-gamecocks-nav",
      "vip": ".story-body",
      "cue": 278142507,
      "filters": {
        "zone.gamecocksNav": true
      }
    },
    {
      "id": "zone-sponsored-article",
      "vip": ".story-body > .header",
      "cue": 277282728,
      "filters": {
        "zone.sponsoredArticle": true
      }
    },
    {
      "id": "zone-lexgoeat-sponsor",
      "vip": ".story-body",
      "cue": 266575046,
      "filters": {
        "zone.lexgoEatSponsor": true
      }
    },
    {
      "id": "zone-disabled-comments",
      "vip": "#zone-el-16",
      "cue": "283807313",
      "filters": {
        "subscriber": true
      }
    },
    {
      "id": "zone-taboola-recommendations",
      "after": "zone-el-101",
      "tracking": true,      
      "filters": {
        "ads": true
      },
      "zephr": {
        "feature": "zone-taboola-recommendations",
        "dataset": ["dma"]
      }
    },
    {
      "id": "zone-gdc-betting-digest",
      "after": "zone-el-101",
      "zephr": {
        "feature": "zone-gdc-betting-digest",
        "dataset": ["marketInfo.domain"]
      },
      "filters": {
        "zone.gdcStoryDigest": true
      },
      "style": "margin: var(--space-lg) auto"
    }
  ]
}
